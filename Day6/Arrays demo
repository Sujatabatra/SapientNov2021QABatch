var arr=[10,20,30];
console.log(arr);
console.log(arr.length);
console.log(arr[1]);
arr[3]="Sujata";
console.log(arr);
arr['myindex']=89;
console.log(arr);
arr[9]=true
console.log(arr);
console.log("===========");
arr.forEach(function(item){
  console.log(item);
})

arr.forEach(function(item,index,array){
  console.log("item - "+item+" - index - "+index+' - array - '+array);
})